    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },

        {
            "name": "Next Train",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts",
            "program": "${workspaceFolder}/charts/next_train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Generate Matplotlib",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts",
            "program": "${workspaceFolder}/charts/inputs/generate_plots/generate_matplotlib.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "--num-images",
                "100",
                // "--no-antialiasing",
                "--scatter",
                "mpl-generated-scatter",
                // "--debug",
            ],
        },
        {
            "name": "Generate OpenCV",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts",
            "program": "${workspaceFolder}/charts/inputs/generate_plots/generate_opencv.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                // "--debug",
                "--num-drawings",
                "5000",
                "--num-images-per-drawing",
                "1",
                "--background-dir",
                "/home/nb/Perso/imagenet/imagenette2",
                "opencv-generated-background"
            ],
        },
        {
            "name": "Evaluate",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts",
            "program": "${workspaceFolder}/charts/dlcharts/evaluation/similar_colors.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "--batch",
                "--model",
                "regression_unetres_v3_scripted.pt",
                // "--baseline",
                "--easy",
                // "--image", "inputs/opencv-generated/img-00001-000.antialiased.png",
            ],
        },
        {
            "name": "Export ONNX",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts",
            "program": "${workspaceFolder}/charts/scripts/export_to_torchscript.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/charts/logs-archive/v4_uresnet18-no-residual_mse_bn32_5e-3/default/checkpoint-00209.pt",
                "output.scripted.pt"
            ],
        },

        {
            "name": "Extract Arxiv",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts/inputs/arxiv/data",
            "program": "${workspaceFolder}/charts/inputs/arxiv/extract_arxiv_figures.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "2203",
            ],
        },

        {
            "name": "dataset_from_figures.py",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/charts/inputs/arxiv",
            "program": "${workspaceFolder}/charts/inputs/arxiv/dataset_from_figures.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "data/selected_pdfs/2203", 
                "arxiv_2203"
            ],
        },
    ]
}